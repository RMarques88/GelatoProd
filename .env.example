# Variables with the EXPO_PUBLIC_ prefix will be embedded in the app bundle.

# Firebase Configuration (required)
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=

# Debug flags (optional)
# Set to 'true' to show banners with Firestore index creation links in development mode
EXPO_PUBLIC_DEBUG_FIRESTORE_INDEX_HELPER=false

# E2E Testing (optional)
# WARNING: Only set to 'true' if you really need to run E2E tests on production (NOT RECOMMENDED)
# Leave empty or set to 'false' for safety
ALLOW_E2E_ON_PROD=false
